
{{form_start(form)}}
<div class="row">
	<div class="col-md-4">{{form_row(form.name)}}</div>
	<div class="col-md-4">{{form_row(form.adress)}}</div>
	<div class="col-md-4">{{form_row(form.description)}}</div>
</div>
<div class="row">
	<div class="col-md-4">{{form_row(form.country)}}</div>
	<div class="col-md-4">{{form_row(form.postal_code)}}</div>
	<div class="col-md-4">{{form_row(form.phone_number)}}</div>
</div>
{{form_rest(form)}}
<button class="btn btn-primary">{{button | default 'Creer'}}</button>
{{form_end(form)}}
